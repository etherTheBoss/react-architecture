    "/Menus": {
      "get": {
        "tags": [
          "Menu Selection"
        ],
        "summary": "Get Menu Selection",
        "parameters": [
          {
            "name": "language",
            "in": "query",
            "description": "language code",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "branchId",
            "in": "query",
            "description": "branch Id",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "depth",
            "in": "query",
            "description": "depth 1= Menu object only, 2= menu with its food_categories, 3= menu, food_category and its items",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
